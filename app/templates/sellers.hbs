<div class="container">
  <div class="row">
    <div class="col-xs-12">
      <h1>Here are all the sellers!</h1>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-12">
      {{new-seller-form saveSeller="saveSeller"}}
    </div>
  </div>
  <div class="row">
    {{#each model as |seller|}}
      {{seller-tile seller=seller}}
    {{/each}}
  </div>
</div>
